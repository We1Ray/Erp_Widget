{
  "compilerOptions": {
    "target": "es6",
    "noEmit": false,
    "noEmitOnError": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./dist/"
  },
  "include": ["next-env.d.ts", "./src/**/*"],
  "exclude": ["./node_modules/"]
}
